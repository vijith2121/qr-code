 "html": "\n{% set slab = {} %}\n{% set subtot = [0] %}\n{% set disc = [0] %}\n{% set taxamt = [0] %}\n<style>\n    .te{font-family: \"Times New Roman\", Times, serif;\n    };\n    .sales{font-family: \"Times New Roman\", Times, serif;}\n</style>\n\n<div class=\"container pt-2\">\n    <div class=\"col-xs-12\">\n            <div class=\"row\">\n                <div class=\"col-xs-4\">\n                    <small class=\"te\">\n                     <b>Head O\ufb03ce:</b><br>\n                        Wahni Green Technologies Pvt.Ltd.<br>\n                        Vikas Trust Rural Industrial Park,<br>\n                        Talikulam P.O,Thrissur-680569<br>\n                        GST No.: 32AABCW8543N1ZX<br>\n                        CIN: U74999KL2016PTC046222\n                    </small>\n                </div>\n                <div class=\"col-xs-4 h-5\"> <h5 class=\"sales\"  ><b>\u0641\u0627\u062a\u0648\u0631\u0629 \u0627\u0644\u0645\u0628\u064a\u0639\u0627\u062a <br>SALES INVOICE </b></h5>\n                        <img class=\"qr-code\" src={{doc.qr_code}} style=\"height:40px;width:40px;margin-left:37px;\">\n                </div>\n              <div class=\"col-xs-4\"><img  style=\"height: 90px;width: 95px;padding-top:25px;\"  src=\"https://wahni.com/assets/website/it/img/wahni-it.png\"></div>\n            </div>\n    </div>\n</div><hr style=\"height: 6px;background-color: #40cbff;width:100%;\">\n\n{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead, footer, print_settings=None) -%}\n\t{% if letter_head and not no_letterhead %}\n\t\t<div class=\"letter-head\" style=\"padding-top: 6px;\">{{ letter_head }}</div>\n\t{% endif %}\n\t{%- if doc.meta.is_submittable and doc.docstatus==0 and (print_settings==None or print_settings.add_draft_heading) -%}\n\t<div class=\"text-center\" document-status=\"draft\">\n\t\t<h4 style=\"margin: 0px;\">{{ _(\"DRAFT\") }}</h4>\n\t</div>\n\t{%- endif -%}\n\t{%- if doc.meta.is_submittable and doc.docstatus==2-%}\n\t<div class=\"text-center\" document-status=\"cancelled\">\n\t\t<h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4>\n\t</div>\n\t{%- endif -%}\n{%- endmacro -%}\n\n{% for page in layout %}\n<div class=\"page-break\">\n\t<div {% if print_settings.repeat_header_footer %} id=\"header-html\" class=\"hidden-pdf\" {% endif %}>\n\t\t{{ add_header(loop.index, layout|len, doc, letter_head, no_letterhead, footer, print_settings) }}\n\t</div>\n\n\t{% if print_settings.repeat_header_footer %}\n\t<div id=\"footer-html\" class=\"visible-pdf\">\n\t\t{% if not no_letterhead and footer %}\n\t\t<div class=\"letter-head-footer\">\n\t\t\t{{ footer }}\n\t\t</div>\n\t\t{% endif %}\n\t\t<p class=\"text-center small page-number visible-pdf\">\n\t\t\t{{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n\t\t</p>\n\t</div>\n\t{% endif %}\n\n    <div class=\"container-fluid\" style=\"min-width: 100% !important; min-height: 225mm !important; font-size: 9px;\">\n        <div class=\"row align-items-start justify-content-start\">\n            <div class=\"col-xs-6\" style=\"font-size: 10px;\">\n                <div class=\"row\">\n                <div class=\"row\">\n                    <div class=\"col-xs-12 \" >\n                        <div class=\"col-xs-5 \"><b>Customer name:<br>\u0627\u0633\u0645 \u0627\u0644\u0632\u0628\u0648\u0646</b></div>\n                        <div class=\"col-xs-7\">{{ doc.customer_name or '-'  }}</div>\n                    </div>\n                    <div class=\"col-xs-12\"style=\"padding-top:10px;\">\n                        <div class=\"col-xs-5 \"><b>Contact:<br>\u0627\u062a\u0635\u0644</b></div>\n                        <div class=\"col-xs-7\">{{ doc.contact_display or '-' }}</div>\n                    </div>\n                    <div class=\"col-xs-12\" style=\"padding-top:10px;\">\n                        <div class=\"col-xs-5\"><b>Billing address:<br>\u0639\u0646\u0648\u0627\u0646 \u0648\u0635\u0648\u0644 \u0627\u0644\u0641\u0648\u0627\u062a\u064a\u0631</b></div>\n                        <div class=\"col-xs-7\">\n                            {{ frappe.db.get_value(\"Address\", doc.customer_address, \"address_line1\") or \"\" }}<br>\n                            {{ frappe.db.get_value(\"Address\", doc.customer_address, \"address_line2\") or \"\" }}<br>\n                            {{ frappe.db.get_value(\"Address\", doc.customer_address, \"city\") or \"\" }} {{ frappe.db.get_value(\"Address\", doc.customer_address, \"pincode\") or \"\" }}<br>\n                            {{ frappe.db.get_value(\"Address\", doc.customer_address, \"state\") or \"\" }} {{ frappe.db.get_value(\"Address\", doc.customer_address, \"country\") or \"\" }}\n                        </div>\n                    </div>\n                </div>\n                </div>\n            </div>\n            <div class=\"col-xs-6 pull-left\" style=\"font-size: 10px;\">\n                <div class=\"row\">\n                <div class=\"row\">\n                    <div class=\"col-xs-12\">\n                        <div class=\"col-xs-5 \"><b>Date:<br>\u062a\u0627\u0631\u064a\u062e</b></div>\n                        <div class=\"col-xs-7\">{{ frappe.utils.formatdate(doc.posting_date, \"dd-mm-yyyy\") }}</div>\n                    </div>\n                    <div class=\"col-xs-12\" style=\"padding-top:10px;\">\n                        <div class=\"col-xs-5\"><b>Invoice No:<br>\u0631\u0642\u0645 \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629</b></div>\n                        <div class=\"col-xs-7\">{{ doc.name or '-' }}</div>\n                    </div>\n                    <div class=\"col-xs-12\" style=\"padding-top:10px;\">\n                        <div class=\"col-xs-5\"><b>VAT NO:<br>\u0636\u0631\u064a\u0628\u0629 \u0627\u0644\u0633\u0644\u0639 \u0648\u0627\u0644\u062e\u062f\u0645\u0627\u062a \u0644\u0627</b></div>\n                        <div class=\"col-xs-7\">{{ doc.tax_id or '' }}</div>\n                    </div>\n                    <div class=\"col-xs-12\" style=\"padding-top:10px;\">\n                        <div class=\"col-xs-5\"><b>PI Ref:<br>\u0645\u0631\u062c\u0639 PI</b></div>\n                        <div class=\"col-xs-7\">{{ doc.items[0].sales_order or '' }}</div>\n                    </div>\n                </div>\n                </div>\n            </div>\n        </div>\n\n\t\t<table class=\"table table-bordered text-wrap\"  style=\"table-layout: fixed;box-shadow: 2px 3px 3px #40cbff;\">\n\t        <thead>\n\t            <tr class=\"table-info\" style=\"font-weight: bold;\">\n\t                <td style=\"width:4%;\">Sr</td>\n\t                <td>Item Description<br>\u0648\u0635\u0641 \u0627\u0644\u0633\u0644\u0639\u0629</td>\n\t                <td style=\"width:9%\">HSN/SAC</td>\n\t                <td style=\"width:7%;\">Qty <br>\u0627\u0644\u0643\u0645\u064a\u0629</td>\n\t                <td class=\"text-right\" style=\"width:10%;\">Unit Rate<br>\u0633\u0639\u0631 \u0627\u0644\u0648\u062d\u062f\u0629</td>\n\t                <td class=\"text-right\" style=\"width:10%;\">Rate (SAR)<br>\u0645\u0639\u062f\u0644</td>\n\t                <td class=\"text-right\" style=\"width:6%;\">\u202cVat.<br>\u202b\u0627\u0644\u0636\u0631\u064a\u0628\u0629\u202c (SAR)</td>\n\t                <td class=\"text-right\" style=\"width:10%;\">Total <br>\u0627\u0644\u0645\u062c\u0645\u0648\u0639<br>\u202b\u202c(SAR)</td>\n\t            </tr>\n\t        </thead>\n\t\t\t<tbody >\n\t\t\t\t{%- for row in doc.items -%}\n\t\t\t\t{% set mydict = json.loads(row.item_tax_rate) %}\n\t\t\t\t{% set gst = [0] %}\n\t\t\t\t{% set tax_value = {} %}\n\t\t\t\t{% for key, value in mydict.items() %}\n\t\t\t\t\t{% if gst.append(gst[-1]+value) %}{% endif %}\n\t\t\t\t\t{% if \"cgst\" in key.lower() %}\n\t\t\t\t\t\t{% if tax_value.update({ \"cgst\": value*row.net_amount/100 }) %}{% endif %}\n\t\t\t\t\t{% endif %}\n\t\t\t\t\t{% if \"sgst\" in key.lower() %}\n\t\t\t\t\t\t{% if tax_value.update({ \"sgst\": value*row.net_amount/100 }) %}{% endif %}\n\t\t\t\t\t{% endif %}\n\t\t\t\t\t{% if \"igst\" in key.lower() %}\n\t\t\t\t\t\t{% if tax_value.update({ \"igst\": value*row.net_amount/100 }) %}{% endif %}\n\t\t\t\t\t{% endif %}\n\t\t\t\t{% endfor %}\n\t\t\t\t{% if slab[(gst[-1])] %}\n\t\t\t\t\t{%- for key, value in tax_value.items() -%}\n\t\t\t\t\t\t{% if slab[(gst[-1])].update({ key: (slab[(gst[-1])][key] + value) }) %}{% endif %}\n\t\t\t\t\t{%- endfor -%}\n\t\t\t\t{% else %}\n\t\t\t\t\t{% if slab.update({ (gst[-1]): tax_value }) %}{% endif %}\n\t\t\t\t{% endif %}\n\t\t\t\t<tr class=\"table-info\">\n\t\t\t\t\t<td>{{ row.idx }}</td>\n\t\t\t\t\t<td>{{ row.description }}</td>\n\t\t\t\t\t<td>{{ row.gst_hsn_code or '' }}</td>\n\t\t\t\t\t<td>{{ row.qty }}{{ row.uom }}</td>\n\t\t\t\t\t<td class=\"text-right\">{{ \"%.2f\"|format(row.net_rate|float) }}</td>\n\t\t\t\t\t<td class=\"text-right\">{{ \"%.2f\"|format(row.net_amount|float) }}</td>\n\t\t\t\t\t<td class=\"text-right\">{{ \"{:,.2f}\".format(gst[-1]|float) }}</td>\n\t\t\t\t\t<td class=\"text-right\">{{ \"{:,.2f}\".format(row.amount|float) }}</td>\n\t\t\t\t</tr>\n\t\t\t\t{%- endfor -%}\n\t\t\t</tbody>\n\t        <tfoot>\n\t            <tr class=\"table-info\">\n\t                <td class=\"text-center\" colspan=\"3\"><b>Total</b></td>\n\t                <td><b>{{ doc.total_qty }}</b></td>\n\t                <td class=\"text-center\"></td>\n\t                <td class=\"text-right\"><b>{{ \"%.2f\"|format(doc.net_total|float) }}</b></td>\n\t                <td class=\"text-right\"></td>\n\t                <td class=\"text-right\"><b>{{ \"%.2f\"|format(doc.rounded_total or doc.grand_total|float) }}</b></td>\n\t            </tr>\n\t            <tr class=\"table-info\">\n\t                <td class=\"text-center\" colspan=\"8\"><b>{{ doc.in_words }}</b></td>\n\t            </tr>\n\t        </tfoot>\n\t\t</table>\n\n\t\t<!--<div class=\"row d-flex align-items-end justify-content-between\">-->\n\t\t<!--    <div class=\"col-xs-9\">-->\n\t\t<!--        <table class=\"table table-bordered text-wrap text-center\" style=\"table-layout: fixed;box-shadow: 2px 3px 3px #40cbff;\">-->\n\t\t<!--            <thead>-->\n\t\t<!--                <tr class=\"table-info\" style=\"font-weight: bold;\">-->\n\t\t<!--                    <td style=\"width:10%\">SI No.</td>-->\n\t\t<!--                    <td>Tax Slab</td>-->\n\t\t<!--                    <td style=\"width:15%\">CGST</td>-->\n\t\t<!--                    <td style=\"width:15%\">SGST</td>-->\n\t\t<!--                    <td style=\"width:15%\">IGST</td>-->\n\t\t<!--                    <td style=\"width:15%\">Total</td>-->\n\t\t<!--                </tr>-->\n\t\t<!--            </thead>-->\n\t\t<!--            <tbody>-->\n\t\t<!--\t\t\t\t{% if not doc.taxes %}-->\n\t <!--                       <tr>-->\n\t <!--                           <td>1</td>-->\n\t <!--                           <td>Nil</td>-->\n\t <!--                           <td>Nil</td>-->\n\t <!--                           <td>Nil</td>-->\n\t <!--                           <td>Nil</td>-->\n\t <!--                           <td>Nil</td>-->\n\t <!--                       </tr>-->\n\t <!--                   {% else %}-->\n\t <!--                       {%- for key, value in slab.items() -%}-->\n\t <!--                           <tr>-->\n\t <!--                               <td>{{ loop.index }}</td>-->\n\t <!--                               <td>{{ key or \"-\" }}%</td>-->\n\t <!--                               <td class=\"text-right\">{{ \"{:,.2f}\".format((value[\"cgst\"] or 0.00)|float) }}</td>-->\n\t <!--                               <td class=\"text-right\">{{ \"{:,.2f}\".format((value[\"sgst\"] or 0.00)|float) }}</td>-->\n\t <!--                               <td class=\"text-right\">{{ \"{:,.2f}\".format((value[\"igst\"] or 0.00)|float) }}</td>-->\n\t <!--                               <td class=\"text-right\">{{ \"{:,.2f}\".format((value.items()|sum(attribute=\"1\") or 0.00